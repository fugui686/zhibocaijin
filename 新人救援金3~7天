        shadow.setBlurRadius(25)
        shadow.setXOffset(0)
        shadow.setYOffset(4)
        shadow.setColor(QColor(0, 0, 0, 80))
        frame.setGraphicsEffect(shadow)

        frame_layout = QVBoxLayout()

        # ===== Base64 logo Â§ÑÁêÜ =====
        logo_base64 = b"""
        
        """  # ‰Ω†ÂèØ‰ª•ÊõøÊç¢‰∏∫‰Ω†Ëá™Â∑±ÁöÑ base64 Â≠óÁ¨¶‰∏≤ÔºàÁ°Æ‰øùÊòØ PNG Êàñ JPGÔºâ

        logo_label = QLabel()
        image_data = base64.b64decode(logo_base64)
        pixmap = QPixmap()
        pixmap.loadFromData(image_data)

        logo_label.setPixmap(pixmap.scaled(80, 80, Qt.KeepAspectRatio, Qt.SmoothTransformation))
        logo_label.setAlignment(Qt.AlignCenter)
        frame_layout.addWidget(logo_label)

        # ÊñáÊú¨ÂÜÖÂÆπ
        about_text = QLabel("""
            <h2 style="text-align:center; color:#2c3e50;">ÂÖ≥‰∫é Â∞èÁáïÂ≠ê</h2>
            <p style="text-align:center; font-size:14px; color:#34495e;">
                <b>Â∞èÁáïÂ≠ê</b> ÊòØÂü∫‰∫é Python ÂºÄÂèëÁöÑËΩªÈáèÁ∫ß <b style="color:#e67e22;">ÁΩëÈ°µÁà¨Ëô´Â∑•ÂÖ∑</b>ÔºåÁî®‰∫éÁªìÊûÑÂåñÊäìÂèñÂä®ÊÄÅÁΩëÁ´ôÊï∞ÊçÆ„ÄÇ
            </p>
            <p style="text-align:center; font-size:14px; color:#34495e;">
                ÊîØÊåÅ <b>Cookie ÁôªÂΩïÊÄÅÁÆ°ÁêÜ</b>„ÄÅ<b>Â§öÁ∫øÁ®ã‰ªªÂä°Âπ∂Âèë</b>„ÄÅ<b>ÂÆöÊó∂Ë∞ÉÂ∫¶ÊâßË°å</b> Âíå <b>ÊåâÂàÜÁªÑÈÖçÁΩÆÊäìÂèñÁ≠ñÁï•</b>„ÄÇ
            </p>
            <p style="text-align:center; font-size:14px; color:#34495e;">
                ÂèØÁî®‰∫éÊé•Âè£Ë∞ÉÁî®ÂàÜÊûê„ÄÅÊï∞ÊçÆËá™Âä®Â§á‰ªΩ„ÄÅËØÑËÆ∫ËøΩË∏™„ÄÅÂÜÖÂÆπÁõëÊéßÁ≠âÂêÑÁ±ªÊï∞ÊçÆÈááÈõÜ‰ªªÂä°„ÄÇ
            </p>
