    def select_file(self):
        desktop_path = QStandardPaths.writableLocation(QStandardPaths.DesktopLocation)
        file_name, _ = QFileDialog.getOpenFileName(self, "ÈÄâÊã©Áî®Êà∑IDÊñá‰ª∂", desktop_path,
                                                   "Text Files (*.txt);;All Files (*)")
        if file_name:
            error_lines = []

            # ‚úÖ ÊØèÊ¨°ÈáçÊñ∞ÂØºÂÖ•ÈÉΩÊ∏ÖÁ©∫ÊóßÊï∞ÊçÆÔºàÈÅøÂÖçÁ¥ØÁßØÔºâ
            self.user_ids = []
            self.user_date_map = {}

            try:
                with open(file_name, 'r', encoding='utf-8') as file:
                    for line_number, line in enumerate(file, start=1):
                        line = line.strip()
                        if line:
                            parts = line.split('\t')
                            if len(parts) >= 2:
                                user_id, raw_date = parts[0].strip(), parts[1].strip()
                                if user_id.isdigit():
                                    try:
                                        parsed_date = datetime.strptime(raw_date, "%Y/%m/%d")
                                        self.user_ids.append(user_id)
                                        self.user_date_map[user_id] = parsed_date.strftime("%Y-%m-%d")
                                    except ValueError:
                                        error_lines.append(line_number)
                                else:
                                    error_lines.append(line_number)
                            else:
                                error_lines.append(line_number)

                if not self.user_ids:
                    QMessageBox.warning(self, "ÈîôËØØ", "Êú™ÊâæÂà∞ÊúâÊïàÁöÑÁî®Êà∑IDÂíåÊó•ÊúüÔºÅ")
                    return

                if error_lines:
                    error_message = f"‰ª•‰∏ãË°åÊ†ºÂºèÊúâËØØÔºàÈùûÊï∞Â≠óÊàñÊó•ÊúüÈîôËØØÔºâÔºö{', '.join(map(str, error_lines))}"
                    QMessageBox.warning(self, "Ê†ºÂºèÈîôËØØ", error_message)
                else:
                    QMessageBox.information(self, "ÊàêÂäü", f"Â∑≤ÊàêÂäüÂä†ËΩΩ {len(self.user_ids)} ‰∏™Áî®Êà∑IDÔºÅ")
                    self.get_button.setEnabled(True)

            except UnicodeDecodeError:
                QMessageBox.warning(self, "ÈîôËØØ", "Êñá‰ª∂ÁºñÁ†ÅÈîôËØØÔºåËØ∑Á°Æ‰øùÊñá‰ª∂‰ΩøÁî®UTF-8ÁºñÁ†ÅÔºÅ")

    def start_processing(self):
        if not hasattr(self, 'user_ids') or not self.user_ids:
            QMessageBox.warning(self, "ÈîôËØØ", "ËØ∑ÂÖàÂØºÂÖ•ÂåÖÂê´Ë¥¶Âè∑ÂíåÊó•ÊúüÁöÑÁî®Êà∑IDÊñá‰ª∂ÔºÅ")
            return

        self.completed_tasks = []
        self.all_3day_data = []
        self.all_7day_data = []

        self.total_summary = {
            "user_count_vaild_bet": 0,
            "user_count_recharge": 0,
            "total_recharge": 0,
            "total_cash": 0,
            "total_valid_bet_amount": 0,
            "diamond_user_count": 0,  # Êñ∞Â¢û
            "diamond_total_sum": 0  # Êñ∞Â¢û
        }

        port = self.port_input.text().strip()
        group = self.group_select_home.currentText()
        token = self.token_inputs[group].text().strip()
        code = self.code_input.text().strip()

        missing_fields = []
        if not port:
            missing_fields.append("Á´ØÂè£ÔºàportÔºâ")
        if not code:
            missing_fields.append("È™åËØÅÁ†ÅÔºàcodeÔºâ")
        if not token:
            missing_fields.append(f"Â∞èÁªÑ {group} ÁöÑ Cookie")

        if missing_fields:
            QMessageBox.warning(self, "ËæìÂÖ•ÈîôËØØ", f"ËØ∑Â°´ÂÜô‰ª•‰∏ãÂÜÖÂÆπÔºö\n" + "\n".join(missing_fields))
            return

        # Á∫øÁ®ãÊï∞
        thread_count = self.thread_count_input.text()
        if thread_count.isdigit() and int(thread_count) > 0:
            max_threads = min(int(thread_count), 100)
            self.thread_pool.setMaxThreadCount(max_threads)
        else:
            QMessageBox.warning(self, "ËæìÂÖ•ÈîôËØØ", "ËØ∑Á°Æ‰øùÂ§öÂºÄÊï∞Èáè‰∏∫Ê≠£Êï¥Êï∞ÔºÅ")
            return

        self.status_label.setText(f"Áä∂ÊÄÅÔºöÊ≠£Âú®Ëé∑ÂèñÊï∞ÊçÆ (0/{len(self.user_ids)})")
        self.progress_bar.setMaximum(len(self.user_ids))
        self.progress_bar.setValue(0)
        self.progress_bar.setVisible(True)

        # Áªü‰∏ÄÂºÄÂßãÊó∂Èó¥‰∏∫Êò®Êó•0ÁÇπ
        yesterday = datetime.now().replace(hour=0, minute=0, second=0, microsecond=0) - timedelta(days=1)
        start_time = int(yesterday.timestamp())

        # ÁªìÊùüÊó∂Èó¥‰∏∫‰ªäÊó•0ÁÇπ
        end_time = int(datetime.now().replace(hour=0, minute=0, second=0, microsecond=0).timestamp())

        for user_id in self.user_ids:
            register_date = self.user_date_map.get(user_id, "")  # Ëé∑ÂèñÊ≥®ÂÜåÊó•Êúü
            signals = WorkerSignals()
            signals.finished.connect(self.task_finished)
            worker = DataWorker(user_id, signals, port, token, code, start_time, end_time, register_date)
            worker.user_id = user_id  # Âõû‰º†Áî®
            self.thread_pool.start(worker)

            self.file_button.setEnabled(False)  # Á¶ÅÁî®ÈÄâÊã©Êñá‰ª∂ÊåâÈíÆ
            self.thread_count_input.setEnabled(False)  # Á¶ÅÁî®Á∫øÁ®ãËæìÂÖ•
            self.group_select_home.setEnabled(False)  # Á¶ÅÁî®Â∞èÁªÑÈÄâÊã©ÈÄâÊã©
            self.get_button.setEnabled(False)  # Á¶ÅÁî®Ëé∑ÂèñÊåâÈíÆ
            self.export_button.setEnabled(False)  # Á¶ÅÁî®ÂØºÂá∫ÊåâÈíÆ

    def task_finished(self, result):  # ‰ªªÂä°ÂÆåÊàêÁöÑÂõûË∞ÉÂáΩÊï∞
        self.completed_tasks.append(result)

        # Á¥ØÂä†ÊÄªÊ±áÊÄªÊï∞ÊçÆ
        self.total_summary["user_count_vaild_bet"] += result.get("user_count_vaild_bet", 0)
        self.total_summary["user_count_recharge"] += result.get("user_count_recharge", 0)
        self.total_summary["total_recharge"] += result.get("total_recharge", 0)
        self.total_summary["total_cash"] += result.get("total_cash", 0)
        self.total_summary["total_valid_bet_amount"] += result.get("total_valid_bet_amount", 0)
        self.total_summary["diamond_user_count"] += result.get("diamond_user_count", 0)
        self.total_summary["diamond_total_sum"] += result.get("diamond_total_sum", 0)

        # ‚úÖ Êî∂ÈõÜ3Â§©/7Â§©Ë¥¶Âè∑Êï∞ÊçÆ
        extra_data = result.get("extra_day_data", {})
        data_3day = extra_data.get("3day", [])
        data_7day = extra_data.get("7day", [])

        # ÂàùÂßãÂåñÂ≠òÂÇ®ÂàóË°®ÔºàÂ¶ÇÊûúËøòÊú™ÂàùÂßãÂåñÔºâ
        if not hasattr(self, 'all_3day_data'):
            self.all_3day_data = []
        if not hasattr(self, 'all_7day_data'):
            self.all_7day_data = []

        self.all_3day_data.extend(data_3day)
        self.all_7day_data.extend(data_7day)

        # Êõ¥Êñ∞ËøõÂ∫¶Êù°
        progress = len(self.completed_tasks)
        total = len(self.user_ids)
        self.progress_bar.setValue(progress)
        self.status_label.setText(f"Áä∂ÊÄÅÔºöÊ≠£Âú®Ëé∑ÂèñÊï∞ÊçÆ ({progress}/{total})")

        # ÊâÄÊúâ‰ªªÂä°ÂÆåÊàêÊó∂
        if progress == total:
            self.progress_bar.setVisible(False)
            self.status_label.setText("Áä∂ÊÄÅÔºöÊâÄÊúâÊï∞ÊçÆÂ∑≤Ëé∑ÂèñÂÆåÊàêÔºÅ")
            # ‚úÖ ÊâìÂç∞ 3Â§©/7Â§©Êï∞ÊçÆÁªüËÆ°Ôºà‰æõË∞ÉËØïÊàñÊó•ÂøóÔºâ
            print(f"‚úÖ ÂÖ±Ëé∑Âèñ 3Â§©Ë¥¶Âè∑Ôºö{len(self.all_3day_data)} ‰∏™")
            print(f"‚úÖ ÂÖ±Ëé∑Âèñ 7Â§©Ë¥¶Âè∑Ôºö{len(self.all_7day_data)} ‰∏™")

            # ÊèêÁ§∫ÂÆåÊàê
            QMessageBox.information(self, "ÊèêÁ§∫", "ÊâÄÊúâÊï∞ÊçÆÂ∑≤Ëé∑ÂèñÂÆåÊàêÔºÅ")

            # ÂêØÁî®ÂêÑÊéß‰ª∂
            self.file_button.setEnabled(True)
            self.thread_count_input.setEnabled(True)
            self.group_select_home.setEnabled(True)
            self.get_button.setEnabled(True)
            self.export_button.setEnabled(True)

    def export_data(self):
        if not self.completed_tasks:
            QMessageBox.warning(self, "Ë≠¶Âëä", "Ê≤°ÊúâÊï∞ÊçÆÂèØÂØºÂá∫ÔºÅ")
            return

        try:
            wb = Workbook()
            wb.remove(wb.active)  # Âà†Èô§ÈªòËÆ§Â∑•‰ΩúË°®

            # ======= ÂéüÊúâÔºöÊï¥ÂêàÊ±áÊÄªË°® =======
            date_grouped = {}
            for result in self.completed_tasks:
                user_id = result.get("user_id")
                if not user_id:
                    continue
                date_str = self.user_date_map.get(user_id, "Êú™Áü•Êó•Êúü")
                if date_str not in date_grouped:
                    date_grouped[date_str] = []
                date_grouped[date_str].append((user_id, result))

            summary_ws = wb.create_sheet(title="Êï¥ÂêàÊ±áÊÄª")
            summary_ws.append(
                ['Ëµ†ÈÄÅ‰∫∫Êï∞', 'Ëµ†ÈÄÅÈáëÈ¢ù', 'ÊâìÁ†Å‰∫∫Êï∞', 'ÂÖÖÂÄº‰∫∫Êï∞', 'ÂèÇ‰∏éÂ∫¶', 'ÂÖÖÂÄº', 'ÊèêÁé∞', 'Áé∞Èáë', '', 'Êó•Êúü', '',
                 'ÊäïÊ≥®‰∫∫Êï∞', 'ÊäïÊ≥®Âç†ÊØî', 'ÈíªÁü≥‰∫∫Êï∞', 'ÈíªÁü≥Âç†ÊØî']
            )
            summary_ws.append([])  # Á©∫Ë°å
            summary_row = 3
            gift_row = 3

            for date_str in sorted(date_grouped.keys()):
                records = date_grouped[date_str]
                gift_user_count = 0
                gift_amount_total = 0
                total_vaild = total_recharge_users = total_recharge = total_cash = total_valid_bet_amount = 0
                diamond_user_count = diamond_total_sum = 0

                for user_id, result in records:
                    total_vaild += result.get("user_count_vaild_bet", 0)
                    total_recharge_users += result.get("user_count_recharge", 0)
                    total_recharge += result.get("total_recharge", 0)
                    total_cash += result.get("total_cash", 0)
                    total_valid_bet_amount += result.get("total_valid_bet_amount", 0)
                    diamond_user_count += result.get("diamond_user_count", 0)
                    diamond_total_sum += result.get("diamond_total_sum", 0)

                    appid_list = result.get("appid_list", [])
                    for entry in appid_list:
                        gift_amount = entry.get("gift_amount", 0)
                        if gift_amount > 0:
                            gift_user_count += 1
                            gift_amount_total += gift_amount

                total_users = len(records)
                cash_balance = total_recharge - total_cash
                participation_rate = (total_vaild / total_users) if total_users else 0
                bet_ratio = (total_vaild / total_users) if total_users else 0
                diamond_ratio = (diamond_user_count / total_users) if total_users else 0

                summary_ws.cell(row=summary_row, column=1, value=gift_user_count)
                summary_ws.cell(row=summary_row, column=2, value=gift_amount_total)
                summary_ws.cell(row=summary_row, column=3, value=total_vaild)
                summary_ws.cell(row=summary_row, column=4, value=total_recharge_users)
                summary_ws.cell(row=summary_row, column=5, value=participation_rate)
                summary_ws.cell(row=summary_row, column=6, value=total_recharge)
                summary_ws.cell(row=summary_row, column=7, value=total_cash)
                summary_ws.cell(row=summary_row, column=8, value=cash_balance)
                summary_ws.cell(row=summary_row, column=10, value=date_str)

                summary_ws.cell(row=summary_row, column=12, value=total_vaild)
                summary_ws.cell(row=summary_row, column=13, value=bet_ratio)
                summary_ws.cell(row=summary_row, column=14, value=diamond_user_count)
                summary_ws.cell(row=summary_row, column=15, value=diamond_ratio)

                summary_ws[f'E{summary_row}'].number_format = "0%"
                summary_ws[f'M{summary_row}'].number_format = "0%"
                summary_ws[f'O{summary_row}'].number_format = "0%"

                for user_id, result in records:
                    appid_list = result.get("appid_list", [])
                    for entry in appid_list:
                        gift_amount = entry.get("gift_amount", 0)
                        if gift_amount > 0:
                            appid = entry.get("appid", 0)
                            app_user_id = entry.get("app_user_id", 0)
                            delta_bet_win = entry.get("delta_bet_win", 0)
                            summary_ws.cell(row=gift_row, column=17, value=int(appid))
                            summary_ws.cell(row=gift_row, column=18, value=int(app_user_id))
                            summary_ws.cell(row=gift_row, column=19, value=gift_amount)
                            summary_ws.cell(row=gift_row, column=20, value=delta_bet_win)
                            gift_row += 1

                summary_row += 2

            # ======= ÂéüÊúâÔºöÊØèÊó•ËØ¶ÊÉÖË°® =======
            for date_str in sorted(date_grouped.keys()):
                records = date_grouped[date_str]
                ws = wb.create_sheet(title=date_str)
                ws.append([
                    'Ëµ†ÈÄÅ‰∫∫Êï∞', 'Ëµ†ÈÄÅÈáëÈ¢ù', 'ÊâìÁ†Å‰∫∫Êï∞', 'ÂÖÖÂÄº‰∫∫Êï∞', 'ÂèÇ‰∏éÂ∫¶', 'ÂÖÖÂÄº', 'ÊèêÁé∞', 'Áé∞Èáë',
                    'Ê∏∏Êàèid', 'ÊúâÊïàÊäïÊ≥®', 'ÂÖÖÂÄºÈáëÈ¢ù', 'ÊèêÁé∞ÈáëÈ¢ù',
                    'Ê∏∏ÊàèÊçüÁõä', 'ÈíªÁü≥‰∫∫Êï∞', 'ÈíªÁü≥ÊÄªÊï∞'
                ])

                gift_user_count = gift_amount_total = 0
                total_vaild = total_recharge_users = total_recharge = total_cash = total_valid_bet_amount = 0
                diamond_user_count = diamond_total_sum = 0

                for user_id, result in records:
                    total_vaild += result.get("user_count_vaild_bet", 0)
                    total_recharge_users += result.get("user_count_recharge", 0)
                    total_recharge += result.get("total_recharge", 0)
                    total_cash += result.get("total_cash", 0)
                    total_valid_bet_amount += result.get("total_valid_bet_amount", 0)
                    diamond_user_count += result.get("diamond_user_count", 0)
                    diamond_total_sum += result.get("diamond_total_sum", 0)

                    appid_list = result.get("appid_list", [])
                    for entry in appid_list:
                        gift_amount = entry.get("gift_amount", 0)
                        if gift_amount > 0:
                            gift_user_count += 1
                            gift_amount_total += gift_amount

                total_users = len(records)
                cash_balance = total_recharge - total_cash
                participation_rate = (total_vaild / total_users) if total_users else 0
                total_delta_bet_win = sum(r.get("delta_bet_win_total", 0) for _, r in records)

                ws.append([
                    gift_user_count, gift_amount_total, total_vaild, total_recharge_users, participation_rate,
                    total_recharge, total_cash, cash_balance,
                    total_users, total_valid_bet_amount, total_recharge, total_cash,
                    total_delta_bet_win, diamond_user_count, diamond_total_sum
                ])
                ws["E2"].number_format = "0%"

                for i, (user_id, result) in enumerate(records, start=3):
                    valid_bet = result.get("total_valid_bet_amount", 0)
                    recharge = result.get("total_recharge", 0)
                    cash = result.get("total_cash", 0)
                    delta_bet_win = result.get("delta_bet_win_total", 0)
                    diamond_total = result.get("diamond_total_sum", 0)
                    ws[f'I{i}'] = int(user_id)
                    ws[f'J{i}'] = int(valid_bet)
                    ws[f'K{i}'] = int(recharge)
                    ws[f'L{i}'] = int(cash)
                    ws[f'M{i}'] = delta_bet_win
                    ws[f'N{i}'] = diamond_user_count
                    ws[f'O{i}'] = diamond_total

            # ======= Èò∂Ê¢ØËµ†ÈÄÅÂáΩÊï∞ =======
            def calculate_reward(game_profit):
                loss_amount = abs(int(game_profit)) if int(game_profit) < 0 else 0
                if loss_amount >= 100000:
                    return 888
                elif loss_amount >= 50000:
                    return 588
                elif loss_amount >= 10000:
                    return 128
                elif loss_amount >= 5000:
                    return 88
                elif loss_amount >= 1000:
                    return 38
                elif loss_amount >= 500:
                    return 18
                elif loss_amount >= 200:
                    return 8
                else:
                    return 0

            # ======= Ë°®1Ôºö3Â§©7Â§©Êï∞ÊçÆÔºà‰∏çÂê´Ëµ†ÈÄÅÂàóÔºâ =======
            if (hasattr(self, 'all_3day_data') and self.all_3day_data) or (
                    hasattr(self, 'all_7day_data') and self.all_7day_data):
                ws = wb.create_sheet(title="3Â§©7Â§©Êï∞ÊçÆ")
                ws["A1"] = "„Äê3Â§©Êï∞ÊçÆ„Äë"
                ws["H1"] = "„Äê7Â§©Êï∞ÊçÆ„Äë"
                ws.append([
                    "Áî®Êà∑ID", "APPID", "ÊúâÊïàÊäïÊ≥®", "ÂÖÖÂÄºÈáëÈ¢ù", "ÊèêÁé∞ÈáëÈ¢ù", "Ê∏∏ÊàèÊçüÁõä", "",
                    "Áî®Êà∑ID", "APPID", "ÊúâÊïàÊäïÊ≥®", "ÂÖÖÂÄºÈáëÈ¢ù", "ÊèêÁé∞ÈáëÈ¢ù", "Ê∏∏ÊàèÊçüÁõä"
                ])

                max_rows = max(len(self.all_3day_data), len(self.all_7day_data))
                for i in range(max_rows):
                    row = []

                    if i < len(self.all_3day_data):
                        item3 = self.all_3day_data[i]
                        row.extend([
                            int(item3.get('user_id', 0)),
                            int(item3.get('app_user_id', 0)),
                            int(item3.get('valid_bet', 0)),
                            int(item3.get('recharge', 0)),
                            int(item3.get('cash', 0)),
                            item3.get('delta_bet_win', 0)
                        ])
                    else:
                        row.extend(["", "", "", "", "", ""])
                    row.append("")

                    if i < len(self.all_7day_data):
                        item7 = self.all_7day_data[i]
                        row.extend([
                            int(item7.get('user_id', 0)),
                            int(item7.get('app_user_id', 0)),
                            int(item7.get('valid_bet', 0)),
                            int(item7.get('recharge', 0)),
                            int(item7.get('cash', 0)),
                            item7.get('delta_bet_win', 0)
                        ])
                    else:
                        row.extend(["", "", "", "", "", ""])
                    ws.append(row)

            # ======= Ë°®2ÔºöËµ†ÈÄÅËÆ∞ÂΩïÊòéÁªÜ =======
            gift_ws = wb.create_sheet(title="Ëµ†ÈÄÅËÆ∞ÂΩïÊòéÁªÜ")
            gift_ws.append(["Ëµ†ÈÄÅÁ±ªÂûã", "Áî®Êà∑ID", "APPID", "Ëµ†ÈÄÅÈáëÈ¢ù", "Ê∏∏ÊàèÊçüÁõä"])

            # 3Â§©Ëµ†ÈÄÅ
            for item in self.all_3day_data:
                profit = item.get('delta_bet_win', 0)
                reward = calculate_reward(profit)
                if reward > 0:
                    gift_ws.append([
                        "3Â§©",
                        int(item.get('user_id', 0)),
                        int(item.get('app_user_id', 0)),
                        reward,
                        profit
                    ])

            # 7Â§©Ëµ†ÈÄÅ
            for item in self.all_7day_data:
                profit = item.get('delta_bet_win', 0)
                reward = calculate_reward(profit)
                if reward > 0:
                    gift_ws.append([
                        "7Â§©",
                        int(item.get('user_id', 0)),
                        int(item.get('app_user_id', 0)),
                        reward,
                        profit
                    ])

            # ======= ‰ªé GÂàóÊèíÂÖ• Ëµ†ÈÄÅÁªüËÆ°Ë°®Ê†ºÔºà3Êó•/7Êó•Ôºâ =======
            reward_steps = [
                (200, 8),
                (500, 18),
                (1000, 38),
                (5000, 88),
                (10000, 128),
                (50000, 588),
                (100000, 888),
            ]

            def summarize_gift_data(data_list):
                summary = {step: {'‰∫∫Êï∞': 0, 'ÂÖÖÂÄº': 0, 'Ëµ†ÈÄÅ': 0} for step, _ in reward_steps}
                for item in data_list:
                    profit = float(item.get('delta_bet_win', 0))
                    if profit >= 0:
                        continue
                    abs_loss = abs(profit)
                    recharge = int(item.get('recharge', 0))
                    reward = calculate_reward(profit)
                    for step, gift in reward_steps:
                        if abs_loss >= step:
                            if gift == reward:
                                summary[step]['‰∫∫Êï∞'] += 1
                                summary[step]['ÂÖÖÂÄº'] += recharge
                                summary[step]['Ëµ†ÈÄÅ'] += gift
                                break
                return summary

            def write_summary(ws, col_start, row_start, title, data_summary, total_users, date_str):
                # ÂêàÂπ∂Ê†áÈ¢òË°åÔºà8ÂàóÔºâ
                ws.merge_cells(start_row=row_start, start_column=col_start, end_row=row_start, end_column=col_start + 7)
                title_cell = ws.cell(row=row_start, column=col_start, value=title)
                title_cell.alignment = Alignment(horizontal="center", vertical="center")

                # Ë°®Â§¥Ë°å
                headers = ["È¶ñÂ≠òÊó•Êúü", "È¶ñÂ≠ò‰∫∫Êï∞", "‰∫èÊçü", "Ëµ†ÈÄÅÂΩ©Èáë", "Ëµ†ÈÄÅÊØî‰æã", "ÂÖÖÂÄº", "Ëµ†ÈÄÅ‰∫∫Êï∞", "Ëµ†ÈÄÅÈáëÈ¢ù"]
                for i, h in enumerate(headers):
                    cell = ws.cell(row=row_start + 1, column=col_start + i, value=h)
                    cell.alignment = Alignment(horizontal="center", vertical="center")

                # Êï∞ÊçÆË°å
                data_start_row = row_start + 2
                total_send = 0
                total_people = 0

                for i, (step, gift) in enumerate(reward_steps):
                    row = data_start_row + i
                    stat = data_summary[step]

                    # ‰∫èÊçü
                    cell = ws.cell(row=row, column=col_start + 2, value=step)
                    cell.alignment = Alignment(horizontal="center", vertical="center")

                    # Ëµ†ÈÄÅÂΩ©Èáë
                    cell = ws.cell(row=row, column=col_start + 3, value=gift)
                    cell.alignment = Alignment(horizontal="center", vertical="center")

                    # Ëµ†ÈÄÅÊØî‰æãÔºàËÆæÁΩÆ‰∏∫ÁôæÂàÜÊØîÊ†ºÂºèÔºâ
                    ratio_cell = ws.cell(row=row, column=col_start + 4, value=gift / step)
                    ratio_cell.number_format = '0.0%'  # ÁôæÂàÜÊØî‰øùÁïô‰∏Ä‰ΩçÂ∞èÊï∞
                    ratio_cell.alignment = Alignment(horizontal="center", vertical="center")

                    # ÂÖÖÂÄº
                    cell = ws.cell(row=row, column=col_start + 5, value=stat['ÂÖÖÂÄº'])
                    cell.alignment = Alignment(horizontal="center", vertical="center")

                    # Ëµ†ÈÄÅ‰∫∫Êï∞
                    cell = ws.cell(row=row, column=col_start + 6, value=stat['‰∫∫Êï∞'])
                    cell.alignment = Alignment(horizontal="center", vertical="center")

                    # Ëµ†ÈÄÅÈáëÈ¢ù
                    cell = ws.cell(row=row, column=col_start + 7, value=stat['Ëµ†ÈÄÅ'])
                    cell.alignment = Alignment(horizontal="center", vertical="center")

                    total_people += stat['‰∫∫Êï∞']
                    total_send += stat['Ëµ†ÈÄÅ']

                # ÂêàÂπ∂È¶ñÂ≠òÊó•Êúü / È¶ñÂ≠ò‰∫∫Êï∞ÂàóÔºàÂûÇÁõ¥ÂêàÂπ∂Ôºâ
                merge_range_date = f"{get_column_letter(col_start)}{data_start_row}:{get_column_letter(col_start)}{data_start_row + len(reward_steps) - 1}"
                merge_range_users = f"{get_column_letter(col_start + 1)}{data_start_row}:{get_column_letter(col_start + 1)}{data_start_row + len(reward_steps) - 1}"
                ws.merge_cells(merge_range_date)
                ws.merge_cells(merge_range_users)

                date_cell = ws.cell(row=data_start_row, column=col_start, value=date_str)
                date_cell.alignment = Alignment(horizontal="center", vertical="center")
                user_cell = ws.cell(row=data_start_row, column=col_start + 1, value=total_users)
                user_cell.alignment = Alignment(horizontal="center", vertical="center")

                # ÂêàËÆ°Ëµ†ÈÄÅË°å
                total_row = data_start_row + len(reward_steps)
                ws.merge_cells(start_row=total_row, start_column=col_start, end_row=total_row, end_column=col_start + 5)
                total_cell = ws.cell(row=total_row, column=col_start, value="ÂêàËÆ°Ëµ†ÈÄÅ")
                total_cell.alignment = Alignment(horizontal="center", vertical="center")

                cell = ws.cell(row=total_row, column=col_start + 6, value=total_people)
                cell.alignment = Alignment(horizontal="center", vertical="center")

                cell = ws.cell(row=total_row, column=col_start + 7, value=total_send)
                cell.alignment = Alignment(horizontal="center", vertical="center")

            # ÊèíÂÖ•Âà∞ GÂàóËµ∑ÔºàÂç≥ column=7Ôºâ
            summary_3 = summarize_gift_data(self.all_3day_data)
            summary_7 = summarize_gift_data(self.all_7day_data)

            date_3day = (datetime.now() - timedelta(days=2)).strftime("%Y/%m/%d")
            date_7day = (datetime.now() - timedelta(days=6)).strftime("%Y/%m/%d")

            write_summary(gift_ws, col_start=7, row_start=1, title="3Êó•ÊÄªÂÖÖÂÄº‰∫èÊçü", data_summary=summary_3,
                          total_users=len(self.all_3day_data), date_str=date_3day)
            write_summary(gift_ws, col_start=7, row_start=13, title="7Êó•ÊÄªÂÖÖÂÄº‰∫èÊçü", data_summary=summary_7,
                          total_users=len(self.all_7day_data), date_str=date_7day)

            # 1. Êï¥ÂêàÊ±áÊÄª‰∏≠ÁöÑËµ†ÈÄÅËÆ∞ÂΩïÔºöÁªüËÆ° app_user_id -> ÈáëÈ¢ù
            combined_gift_data = defaultdict(int)
            summary_ws = wb["Êï¥ÂêàÊ±áÊÄª"]
            for row in summary_ws.iter_rows(min_row=3):
                app_user_id = row[17].value  # RÂàóÔºàÁ¨¨18ÂàóÔºâ
                gift_amount = row[18].value  # SÂàóÔºàÁ¨¨19ÂàóÔºâ
                if app_user_id is not None and gift_amount is not None:
                    try:
                        app_user_id = int(app_user_id)
                        gift_amount = float(gift_amount)
                        combined_gift_data[app_user_id] += gift_amount
                    except:
                        continue

            # 2. ÈÅçÂéÜËµ†ÈÄÅËÆ∞ÂΩïÊòéÁªÜÔºåÂêàÂπ∂ÈáëÈ¢ùÔºåÂÜôÂÖ• P/Q ÂàóÔºåÂêåÊó∂ËÆ∞ÂΩïÂ∑≤Â§ÑÁêÜÁöÑ app_user_id
            gift_ws = wb["Ëµ†ÈÄÅËÆ∞ÂΩïÊòéÁªÜ"]
            processed_ids = set()

            for i, row in enumerate(gift_ws.iter_rows(min_row=2), start=2):
                app_user_id_cell = row[2]  # CÂàóÔºàÁ¨¨3ÂàóÔºâ
                gift_amount_cell = row[3]  # DÂàóÔºàÁ¨¨4ÂàóÔºâ

                if app_user_id_cell.value is not None:
                    try:
                        app_user_id = int(app_user_id_cell.value)
                        original_gift = float(gift_amount_cell.value or 0)
                        extra_gift = combined_gift_data.get(app_user_id, 0)
                        total_gift = original_gift + extra_gift

                        # ÂÜôÂÖ• P/Q Âàó
                        gift_ws.cell(row=i, column=16, value=app_user_id)  # PÂàó
                        gift_ws.cell(row=i, column=17, value=total_gift)  # QÂàó

                        processed_ids.add(app_user_id)
                    except:
                        continue

            # 3. ËøΩÂä†Êï¥ÂêàÊ±áÊÄª‰∏≠‚ÄúÊú™Âá∫Áé∞Âú®Ëµ†ÈÄÅËÆ∞ÂΩïË°®‚ÄùÁöÑ app_user_id
            next_row = gift_ws.max_row + 1
            for app_user_id, extra_gift in combined_gift_data.items():
                if app_user_id not in processed_ids:
                    gift_ws.cell(row=next_row, column=16, value=app_user_id)
                    gift_ws.cell(row=next_row, column=17, value=extra_gift)
                    next_row += 1

            # ======= ‰øùÂ≠òÊñá‰ª∂ =======
            filename = f"ÂèÇ‰∏éÂ∫¶_{datetime.now().strftime('%YÂπ¥%mÊúà%dÊó•%HÊó∂%MÂàÜ%SÁßí')}.xlsx"
            wb.save(filename)
            QMessageBox.information(self, "ÊèêÁ§∫", f"Êï∞ÊçÆÂ∑≤ÂØºÂá∫Âà∞ {filename}")
            os.startfile(filename)

        except Exception as e:
            QMessageBox.critical(self, "ÂØºÂá∫ÈîôËØØ", f"ÂØºÂá∫Êï∞ÊçÆÊó∂ÂèëÁîüÈîôËØØ: {str(e)}")

    def load_group_names_from_config(self):
        config = ConfigParser()
        config.read('config.ini')
        if 'GROUP_COOKIES' in config:
            return list(config['GROUP_COOKIES'].keys())
        return ["ÁªøËå∂", "Â¶ñÂß¨", "Ëâ≤Êàí", "Áà±Ëâ≤Êí≠", "ÊÄßÁ¶è", "Â∞ëÂ•≥", "‰∫∫Â¶ª", "‰πêÂí™"]

    def save_config(self, port, token_dict, code):
        config = ConfigParser()
        config['COMMON'] = {'Port': port, 'Code': code}
        config['GROUP_COOKIES'] = {}
        for group, token in token_dict.items():
            config['GROUP_COOKIES'][group] = token

        with open('config.ini', 'w') as f:
            config.write(f)

    def load_config(self):
        config = ConfigParser()
        config.read('config.ini')

        if 'COMMON' in config:
            self.port_input.setText(config['COMMON'].get('Port', ''))
            self.code_input.setText(config['COMMON'].get('Code', ''))

        if 'GROUP_COOKIES' in config:
            for group in self.groups:
                if group in config['GROUP_COOKIES']:
                    self.token_inputs[group].setText(config['GROUP_COOKIES'].get(group, ''))

    def manual_save(self):  # ÊâãÂä®‰øùÂ≠òÈÖçÁΩÆ
        port = self.port_input.text()
        code = self.code_input.text()

        token_dict = {}
        missing_groups = []

        for group, input_field in self.token_inputs.items():
            token = input_field.text()
            if token:
                token_dict[group] = token
                # ËÆæÁΩÆ‰∏∫Ê≠£Â∏∏Áä∂ÊÄÅÔºàÊ∏ÖÈô§Á∫¢Ëâ≤ËæπÊ°ÜÔºâ
                input_field.setStyleSheet("")
            else:
                missing_groups.append(group)
                # ËÆæÁΩÆÁ∫¢Ëâ≤ËæπÊ°Ü
                input_field.setStyleSheet("border: 1px solid red;")

        if not port or not code:
            QMessageBox.warning(self, "Ë≠¶Âëä", "ËØ∑Â°´ÂÜôÂÆåÊï¥ÁöÑÈÖçÁΩÆÔºàportÂíåcodeÔºâÔºÅ")
            return

        self.save_config(port, token_dict, code)

        if missing_groups:
            missing_text = "„ÄÅ".join(missing_groups)
            QMessageBox.information(self, "ÈÉ®ÂàÜ‰øùÂ≠òÊàêÂäü", f"ÈÖçÁΩÆÂ∑≤‰øùÂ≠òÔºå‰ΩÜ‰ª•‰∏ãÂ∞èÁªÑÊú™Â°´ÂÜô CookieÔºö{missing_text}")
        else:
            QMessageBox.information(self, "ÊèêÁ§∫", "ÈÖçÁΩÆÂ∑≤ÂÆåÊï¥‰øùÂ≠òÔºÅ")


if __name__ == "__main__":
    app = QApplication(sys.argv)
    window = UserWindow()
    window.show()
    sys.exit(app.exec_())
