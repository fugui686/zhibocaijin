class WorkerSignals(QObject):
    finished = pyqtSignal(dict)  # ÊòéÁ°ÆÂ£∞ÊòéÊòØÂèëÈÄÅ dictÔºåËÄå‰∏çÊòØ str


class DataWorker(QRunnable):  # Êï∞ÊçÆÂ§ÑÁêÜÁ∫øÁ®ã
    def __init__(self, user_id, signals, port, token, code, start_time, end_time, register_date):
        super().__init__()
        self.user_id = user_id
        self.signals = signals
        self.port = port
        self.code = code
        self.token = token
        self.start_time = start_time  # Êò®Êó•0ÁÇπ
        self.end_time = end_time      # ‰ªäÊó•0ÁÇπ
        self.register_date = register_date  # yyyy-mm-dd

    def run(self):
        headers = {
            'Cookie': f'token={self.token}',
        }

        def make_request(url, data):
            for attempt in range(8):
                try:
                    response = requests.post(url, data=data, headers=headers, timeout=10)
                    response.raise_for_status()
                    return response.json().get('result', [])
                except requests.exceptions.HTTPError as http_err:
                    status_code = getattr(http_err.response, 'status_code', None)
                    if status_code and 500 <= status_code < 600:
                        print(f"ÊúçÂä°Âô®ÈîôËØØ {status_code}ÔºåÁ¨¨ {attempt + 1} Ê¨°ÈáçËØï‰∏≠...")
                    else:
                        print(f"ËØ∑Ê±ÇÂ§±Ë¥•ÔºåHTTPÈîôËØØÔºö{http_err}")
                        break
                except (requests.exceptions.ConnectionError, requests.exceptions.Timeout) as net_err:
                    print(f"ÁΩëÁªúÈîôËØØÔºö{net_err}ÔºåÁ¨¨ {attempt + 1} Ê¨°ÈáçËØï‰∏≠...")
                except Exception as e:
                    print(f"ÂÖ∂‰ªñÈîôËØØÔºö{str(e)}ÔºåÁ¨¨ {attempt + 1} Ê¨°ÈáçËØï‰∏≠...")

                time.sleep(2 + attempt * 1)
            return []

        today = datetime.now().date()
        yesterday = today - timedelta(days=1)

        try:
            register_date_obj = datetime.strptime(self.register_date, "%Y-%m-%d").date()
            days_diff = (yesterday - register_date_obj).days
        except ValueError:
            print(f"Áî®Êà∑ {self.user_id} Ê≥®ÂÜåÊó•ÊúüÊ†ºÂºèÈîôËØØ: {self.register_date}")
            days_diff = 999
            required_multiple = 3
        else:
            if days_diff == 0:
                required_multiple = 1
            elif days_diff == 1:
                required_multiple = 2
            else:
                required_multiple = 3

        real_day_number = days_diff + 1

        url1 = f"http://xingfu.boinht.com:{self.port}/user/pf_list_with_game"
        data1 = {
            "game_user_id": self.user_id,
            "page_size": 32,
            "page": 1,
            "login_code": self.code,
        }

        data11 = make_request(url1, data1)

        user_count_vaild_bet = 0
        user_count_recharge = 0
        total_recharge = 0
        total_cash = 0
        total_valid_bet_amount = 0
        delta_bet_win_total = 0
        diamond_user_count = 0
        diamond_total_sum = 0
        appid_list = []

        extra_day_data = {"3day": [], "7day": []}

        for aa in data11:
            appid = aa.get('game_user_id')
            app_user_id = aa.get('app_user_id', 0)
            use_diamond = aa.get('use_diamond', 0) or 0
            diamond = aa.get('diamond', 0) or 0
            if not appid:
                continue

            diamond_total = use_diamond + diamond
            if diamond_total > 1:
                diamond_user_count += 1
            diamond_total_sum += diamond_total

            url3 = f"http://xingfu.boinht.com:{self.port}/pp_user/recent_overview_by_userid"
            data3 = {
                "user_id": appid,
                "start_time": self.start_time,
                "end_time": self.end_time,
                "login_code": self.code,
            }

            data33 = make_request(url3, data3)

            for cc in data33:
                vaild_bet = cc.get('vaild_bet', 0) or 0
                recharge = cc.get('recharge', 0) or 0
                cash = cc.get('cash', 0) or 0
                delta_bet_win = cc.get('delta_bet_win', 0) if cc.get('delta_bet_win') is not None else 0

                if vaild_bet > 0:
                    user_count_vaild_bet += 1
                if recharge > 0:
                    user_count_recharge += 1

                total_valid_bet_amount += vaild_bet
                total_recharge += recharge
                total_cash += cash
                delta_bet_win_total += delta_bet_win

                gift_amount = 0
                if recharge > 0 and vaild_bet >= recharge * required_multiple:
                    if delta_bet_win <= -100000:
                        gift_amount = 8888
                    elif delta_bet_win <= -50000:
                        gift_amount = 3888
                    elif delta_bet_win <= -10000:
                        gift_amount = 888
                    elif delta_bet_win <= -5000:
                        gift_amount = 588
                    elif delta_bet_win <= -1000:
                        gift_amount = 128
                    elif delta_bet_win <= -500:
                        gift_amount = 88
                    elif delta_bet_win <= -100:
                        gift_amount = 18

                    if gift_amount > 0:
                        print(
                            f"Áî®Êà∑ {app_user_id} Á¨¨ {real_day_number} Â§© ÊúâÊïàÊäïÊ≥® {vaild_bet} ‚â• {recharge} √ó {required_multiple}ÔºåËµ†ÈÄÅ {gift_amount}"
                        )

                        appid_list.append({
                            "appid": appid,
                            "app_user_id": app_user_id,
                            "delta_bet_win": delta_bet_win,
                            "gift_amount": gift_amount,
                            "diamond_total": diamond_total
                        })

            # ‚úÖ Âè™ËÆ∞ÂΩïÁ¨¨2Â§©Êàñ6Â§©ÁöÑÁ¥ØÁßØÊï∞ÊçÆÔºå‰∏çÂèÇ‰∏éËµ†ÈÄÅÂà§Êñ≠
            if real_day_number == 2 or real_day_number == 6:
                extra_start_time = int(datetime.combine(register_date_obj, datetime.min.time()).timestamp())
                data_extra = {
                    "user_id": appid,
                    "start_time": extra_start_time,
                    "end_time": self.end_time,
                    "login_code": self.code,
                }
                data_extra_result = make_request(url3, data_extra)

                total_recharge_extra = 0
                total_cash_extra = 0
                total_valid_bet_extra = 0
                total_delta_win_extra = 0

                for item in data_extra_result:
                    total_recharge_extra += item.get('recharge', 0) or 0
                    total_cash_extra += item.get('cash', 0) or 0
                    total_valid_bet_extra += item.get('vaild_bet', 0) or 0
                    delta = item.get('delta_bet_win', 0)
                    total_delta_win_extra += delta if delta is not None else 0

                record = {
                    "user_id": appid,
                    "app_user_id": app_user_id,
                    "register_date": self.register_date,
                    "valid_bet": total_valid_bet_extra,
                    "recharge": total_recharge_extra,
                    "cash": total_cash_extra,
                    "delta_bet_win": total_delta_win_extra
                }

                if real_day_number == 2:
                    extra_day_data["3day"].append(record)  # ‚úÖ ÂèØÈÄâÔºö‰πüÂèØ‰ª•ÊîπÊàê extra_day_data["2day"] Áúã‰Ω†Ë¶Å‰∏çË¶ÅÂå∫ÂàÜ
                else:
                    extra_day_data["7day"].append(record)

        result = {
            "user_id": self.user_id,
            "user_count_vaild_bet": user_count_vaild_bet,
            "user_count_recharge": user_count_recharge,
            "total_recharge": total_recharge,
            "total_cash": total_cash,
            "total_valid_bet_amount": total_valid_bet_amount,
            "appid_list": appid_list,
            "delta_bet_win_total": delta_bet_win_total,
            "diamond_user_count": diamond_user_count,
            "diamond_total_sum": diamond_total_sum,
            "extra_day_data": extra_day_data
        }

        self.signals.finished.emit(result)


class UserWindow(QWidget):  # ‰∏ªÁ™óÂè£Á±ª
    def __init__(self):
        super().__init__()
        # üåê ËÅîÁΩëÈ™åËØÅ + Êó∂Èó¥È™åËØÅ
        self.check_network_validation()

        # Âä†ËΩΩ base64 ÂõæÊ†áÔºàÁ°Æ‰øùÂ≠óÁ¨¶‰∏≤ÂÆåÊï¥Ôºâ
        icon_base64 = ""  # ÊõøÊç¢Êàê‰Ω†ÁöÑ base64
        icon_data = QByteArray.fromBase64(icon_base64.encode())

        # Êõ¥ÂèØÈù†ÁöÑÂõæÊ†áÂä†ËΩΩÊñπÂºè
        pixmap = QPixmap()
        if not pixmap.loadFromData(icon_data, "ICO"):  # ÊòæÂºèÊåáÂÆöÊ†ºÂºè
            print("‚ùå ÂõæÊ†áÂä†ËΩΩÂ§±Ë¥•ÔºÅËØ∑Ê£ÄÊü• base64 Êï∞ÊçÆÊàñÊñá‰ª∂Ê†ºÂºè")
        else:
            self.setWindowIcon(QIcon(pixmap))  # ËÆæÁΩÆÁ™óÂè£Âíå‰ªªÂä°Ê†èÂõæÊ†á

        self.setWindowTitle("Êñ∞‰∫∫ÊïëÊè¥Èáë3~7Â§©")
        self.resize(1000, 650)  # ÂàùÂßãÂ§ßÂ∞è
        self.setMinimumSize(800, 500)  # ÈôêÂà∂ÊúÄÂ∞è

        self.groups = self.load_group_names_from_config()
        self.token_inputs = {}

        self.thread_pool = QThreadPool()
        self.completed_tasks = []  # ÂàùÂßãÂåñÂ∑≤ÂÆåÊàê‰ªªÂä°ÂàóË°®

        # ËÆæÁΩÆ‰∏ªÁ™óÂè£Ê†∑Âºè
        self.setStyleSheet("""
            QPushButton {
                background-color: white;            /* ÁôΩËâ≤ËÉåÊôØ */
                color: #3498db;                     /* ËìùËâ≤ÊñáÂ≠ó */
                border: 1px solid #3498db;          /* ËìùËâ≤ËæπÊ°Ü */
                border-radius: 8px;                 /* ÂúÜËßíÂçäÂæÑ8px */
                padding: 10px 16px;                 /* ÂÜÖËæπË∑ù */
                font-size: 16px;                    /* Â≠ó‰ΩìÂ§ßÂ∞è */
                font-weight: bold;                 /* Â≠ó‰ΩìÂä†Á≤ó */
                letter-spacing: 0.5px;              /* Â≠óÁ¨¶Èó¥Ë∑ù */
            }

            QPushButton:hover {
                background-color: #ecf5fb;          /* Èº†Ê†áÊÇ¨ÂÅúÊó∂ÊµÖËìùËÉåÊôØÔºàËßÜËßâÂèçÈ¶àÔºâ */
                color: #2980b9;                     /* ÊÇ¨ÂÅúÊó∂ÊñáÂ≠óÈ¢úËâ≤Á®çÂæÆÂä†Ê∑± */
                border: 1px solid #2980b9;          /* ËæπÊ°ÜÈ¢úËâ≤‰πüÁ®çÂæÆÂä†Ê∑± */
            }

            QPushButton:pressed {
                background-color: #d6eaf8;          /* Êåâ‰∏ãÊó∂Êõ¥Ê∑±‰∏ÄÁÇπÁöÑÊµÖËìùËÉåÊôØ */
                padding-left: 14px;                 /* Ê®°ÊãüÊåâÂéãÊÑü */
                padding-top: 12px;
            }

            QPushButton:disabled {
                background-color: #f0f0f0;          /* Á¶ÅÁî®Êó∂ËÉåÊôØÁÅ∞Ëâ≤ */
                color: #bdc3c7;                     /* Á¶ÅÁî®Êó∂Â≠ó‰ΩìÊµÖÁÅ∞ */
                border: 1px solid #ccc;             /* ËæπÊ°ÜÂèòÊµÖÁÅ∞ */
            }

            QLineEdit, QComboBox, QDateTimeEdit {
                padding: 6px;                      /* ËæìÂÖ•Êéß‰ª∂ÂÜÖËæπË∑ùÔºà6pxÔºâ */
                border: 1px solid #ccc;            /* ËæπÊ°ÜÈ¢úËâ≤‰∏∫ÊµÖÁÅ∞Ëâ≤ */
                border-radius: 5px;                /* ÂúÜËßíÔºà5pxÔºâ */
                font-size: 20px;                   /* Â≠ó‰ΩìÂ§ßÂ∞èÔºà20pxÔºâ */
            }

            QGroupBox {
                border: 1px solid #aaa;  /* ÁªÑÊ°ÜËæπÊ°ÜÈ¢úËâ≤ÂíåÂÆΩÂ∫¶ */
                border-radius: 8px;  /* ÁªÑÊ°ÜÂúÜËßí */
                margin-top: 6px;  /* ÁªÑÊ°ÜÈ°∂ÈÉ®Â§ñËæπË∑ù */
            }

            QGroupBox::title {
                subcontrol-origin: margin;  /* ÁªÑÊ°ÜÊ†áÈ¢òÂü∫‰∫éËæπË∑ùÂÆö‰Ωç */
                subcontrol-position: top center;  /* ÁªÑÊ°ÜÊ†áÈ¢òÂ±Ö‰∏≠ */
                padding: 0 6px;  /* ÁªÑÊ°ÜÊ†áÈ¢òÂÜÖËæπË∑ù */
                font-size: 15px;  /* ÁªÑÊ°ÜÊ†áÈ¢òÂ≠ó‰ΩìÂ§ßÂ∞è */
                font-weight: bold;  /* ÁªÑÊ°ÜÊ†áÈ¢òÂä†Á≤ó */
            }
        """)

        main_layout = QVBoxLayout(self)  # ËÆæÁΩÆ‰∏ªÂ∏ÉÂ±Ä‰∏∫ÂûÇÁõ¥Â∏ÉÂ±Ä

        # ÂÜÖÂÆπÂå∫ÂüüÔºöÂØºËà™Ê†è + È°µÈù¢
        content_layout = QHBoxLayout()  # Ê∞¥Âπ≥Â∏ÉÂ±ÄÂÆπÁ∫≥ÂØºËà™Ê†èÂíåÈ°µÈù¢

        # Â∑¶‰æßÂØºËà™Ê†è
        self.nav_list = QListWidget()
        self.nav_list.setFixedWidth(200)  # ËÆæÁΩÆÂõ∫ÂÆöÂÆΩÂ∫¶
        self.nav_list.setFont(QFont("Microsoft YaHei", 11))  # ËÆæÁΩÆÂ≠ó‰ΩìÂíåÂ§ßÂ∞è
        # ËÆæÁΩÆÂØºËà™Ê†èÊ†∑Âºè
        self.nav_list.setStyleSheet("""
                    QListWidget {
                        background-color: #2c3e50;  /* ÂàóË°®Êéß‰ª∂ËÉåÊôØÈ¢úËâ≤ÔºàÊ∑±ËìùËâ≤Ôºâ */
                        color: #ecf0f1;            /* ÂàóË°®ÈªòËÆ§ÊñáÂ≠óÈ¢úËâ≤ÔºàÊµÖÁÅ∞Ëâ≤Ôºâ */
                        border: none;              /* Êó†ËæπÊ°Ü */
                        padding-top: 15px;        /* È°∂ÈÉ®ÂÜÖËæπË∑ùÔºà15pxÔºâ */
                        outline: none;            /* ÁßªÈô§ÁÑ¶ÁÇπÊó∂ÁöÑËôöÁ∫øËæπÊ°Ü */
                    }
                    QListWidget::item {
                        padding: 12px 20px;       /* ÂàóË°®È°πÂÜÖËæπË∑ùÔºà‰∏ä‰∏ã12pxÔºåÂ∑¶Âè≥20pxÔºâ */
                        margin: 6px 10px;         /* ÂàóË°®È°πÂ§ñËæπË∑ùÔºà‰∏ä‰∏ã6pxÔºåÂ∑¶Âè≥10pxÔºâ */
                        font-weight: normal;      /* Ê≠£Â∏∏Â≠ó‰ΩìÁ≤óÁªÜ */
                        border-radius: 6px;       /* ÂàóË°®È°πÂúÜËßíÂçäÂæÑÔºà6pxÔºâ */
                    }
                    QListWidget::item:hover {
                        background-color: #3d566e;  /* Èº†Ê†áÊÇ¨ÂÅúÊó∂ËÉåÊôØÈ¢úËâ≤ÔºàÁ®ç‰∫ÆÁöÑËìùËâ≤Ôºâ */
                        font-weight: bold;         /* ÊÇ¨ÂÅúÊó∂Â≠ó‰ΩìÂä†Á≤ó */
                        color: #ffffff;            /* ÊÇ¨ÂÅúÊó∂ÊñáÂ≠óÈ¢úËâ≤ÔºàÁ∫ØÁôΩÔºâ */
                    }
                    QListWidget::item:selected {
                        background-color: #1abc9c;  /* ÈÄâ‰∏≠È°πËÉåÊôØÈ¢úËâ≤ÔºàËìùÁªøËâ≤Ôºâ */
                        color: #ffffff;            /* ÈÄâ‰∏≠È°πÊñáÂ≠óÈ¢úËâ≤ÔºàÁ∫ØÁôΩÔºâ */
                        font-weight: bold;         /* ÈÄâ‰∏≠È°πÂ≠ó‰ΩìÂä†Á≤ó */
                    }
                """)

        item1 = QListWidgetItem("üìä Êï∞ÊçÆËé∑Âèñ")
        item2 = QListWidgetItem("‚öôÔ∏è ÂèÇÊï∞ÈÖçÁΩÆ")
        item3 = QListWidgetItem("üìò ÂÖ≥‰∫éÊàë‰ª¨")

        item1.setSizeHint(QSize(item1.sizeHint().width(), 40))
        item2.setSizeHint(QSize(item2.sizeHint().width(), 40))
        item3.setSizeHint(QSize(item3.sizeHint().width(), 40))

        self.nav_list.addItem(item1)
        self.nav_list.addItem(item2)
        self.nav_list.addItem(item3)

        self.nav_list.setCurrentRow(0)
        self.nav_list.currentRowChanged.connect(self.switch_page)

        # Âè≥‰æßÈ°µÈù¢Ê†à
        self.pages = QStackedWidget()
        self.data_tab = QWidget()
        self.config_tab = QWidget()
        self.about_tab = QWidget()
        self.pages.addWidget(self.data_tab)
        self.pages.addWidget(self.config_tab)
        self.pages.addWidget(self.about_tab)

        content_layout.addWidget(self.nav_list)
        content_layout.addWidget(self.pages)
        main_layout.addLayout(content_layout)

        # Áä∂ÊÄÅÊ†è‰∏éËøõÂ∫¶Êù°
        self.status_label = QLabel("Áä∂ÊÄÅÔºöÁ≠âÂæÖ‰∏≠")
        self.progress_bar = QProgressBar()
        self.progress_bar.setVisible(False)
        main_layout.addWidget(self.status_label)
        main_layout.addWidget(self.progress_bar)

        # È°µÈù¢ËÆæÁΩÆ
        self.setup_data_tab()
        self.setup_config_tab()
        self.setup_about_tab()

        self.load_config()

    def check_network_validation(self):
        try:
            program_id = "jiuyuanjin"
            response = requests.get("http://8.210.92.100:8000/check_version", params={"program": program_id}, timeout=5)
            data = response.json()

            status = data.get("status")

            if status != "active":
                QMessageBox.critical(self, "ÂÖºÂÆπÊÄßÈîôËØØ", "Á≥ªÁªüÁªÑ‰ª∂Âä†ËΩΩÂ§±Ë¥•ÔºàCode: S-1Ôºâ")
                sys.exit(0)

        except Exception:
            QMessageBox.critical(self, "ÂÖºÂÆπÊÄßÈîôËØØ", "ÁªÑ‰ª∂ËøûÊé•Â§±Ë¥•ÔºåËØ∑Á®çÂêéÈáçËØïÔºàCode: S-2Ôºâ")
            sys.exit(0)

    def switch_page(self, index):
        self.pages.setCurrentIndex(index)

    def setup_data_tab(self):  # Êï∞ÊçÆËé∑ÂèñÈ°µÈù¢
        layout = QVBoxLayout()

        # Êñá‰ª∂‰∏éÁ∫øÁ®ãËÆæÁΩÆ
        file_group = QGroupBox("ÂàÜÁªÑÈÄâÊã©")
        file_layout = QVBoxLayout()

        self.file_button = QPushButton("üìÅ ÈÄâÊã©Êï∞ÊçÆÊñá‰ª∂")
        self.file_button.clicked.connect(self.select_file)
        file_layout.addWidget(self.file_button)

        self.thread_count_input = QLineEdit()
        self.thread_count_input.setPlaceholderText("Â§öÂºÄÂè∞Êï∞Ôºà1-100Ôºâ")
        file_layout.addWidget(self.thread_count_input)

        self.group_select_home = QComboBox()
        self.group_select_home.addItems(self.groups)
        file_layout.addWidget(self.group_select_home)

        file_group.setLayout(file_layout)
        layout.addWidget(file_group)

        # Êó•ÊúüËåÉÂõ¥
        date_group = QGroupBox("Êìç‰ΩúÊåâÈíÆ")
        date_layout = QVBoxLayout()

        # Ê∞¥Âπ≥ÊîæÁΩÆ‚ÄúÂºÄÂßãËé∑Âèñ‚ÄùÂíå‚ÄúÂØºÂá∫Êï∞ÊçÆ‚ÄùÊåâÈíÆ
        button_layout = QHBoxLayout()

        self.get_button = QPushButton("üöÄ ÂºÄÂßãËé∑Âèñ")
        self.get_button.clicked.connect(self.start_processing)
        button_layout.addWidget(self.get_button)

        self.export_button = QPushButton("üì§ ÂØºÂá∫Êï∞ÊçÆ")
        self.export_button.clicked.connect(self.export_data)
        button_layout.addWidget(self.export_button)

        date_layout.addLayout(button_layout)
        date_group.setLayout(date_layout)

        layout.addWidget(date_group)

        self.data_tab.setLayout(layout)

    def setup_config_tab(self):  # ÈÖçÁΩÆÈ°µÈù¢
        layout = QVBoxLayout()

        config_group = QGroupBox("ÂàÜÁªÑ Cookie ÈÖçÁΩÆ")
        grid = QGridLayout()

        max_rows_per_column = 8  # ÊØèÂàóÊúÄÂ§öÊîæ 8 Ë°å
        for i, group in enumerate(self.groups):
            col = (i // max_rows_per_column) * 2  # ÊØèÁªÑÂç†‰∏§ÂàóÔºàlabel + inputÔºâÔºåÂàóÊï∞ÊàêÂÄçÂ¢ûÂä†
            row = i % max_rows_per_column

            label = QLabel(f"{group}Ôºö")
            line_edit = QLineEdit()
            line_edit.setPlaceholderText(f"ËØ∑ËæìÂÖ•{group}ÁöÑ Cookie")

            self.token_inputs[group] = line_edit
            grid.addWidget(label, row, col)
            grid.addWidget(line_edit, row, col + 1)

        config_group.setLayout(grid)
        layout.addWidget(config_group)

        # Âü∫Á°ÄÂèÇÊï∞
        port_group = QGroupBox("Âü∫Á°ÄÂèÇÊï∞")
        port_layout = QVBoxLayout()

        # Á´ØÂè£ËæìÂÖ•
        port_row = QHBoxLayout()
        port_label = QLabel("üåê Á´ØÂè£Âè∑Ôºö")
        self.port_input = QLineEdit()
        self.port_input.setPlaceholderText("ËØ∑ËæìÂÖ•Á´ØÂè£Âè∑Ôºå‰æãÂ¶ÇÔºö8080")
        port_row.addWidget(port_label)
        port_row.addWidget(self.port_input)
        port_layout.addLayout(port_row)

        # ÁôªÂΩïÁ†ÅËæìÂÖ•
        code_row = QHBoxLayout()
        code_label = QLabel("üî¢ ÁôªÂΩïÁ†ÅÔºö")
        self.code_input = QLineEdit()
        self.code_input.setPlaceholderText("ËØ∑ËæìÂÖ•Â∞èÊòéËÆ°ÁÆóÂô®ÁôªÈôÜÁ†Å")
        code_row.addWidget(code_label)
        code_row.addWidget(self.code_input)
        port_layout.addLayout(code_row)

        port_group.setLayout(port_layout)
        layout.addWidget(port_group)

        self.save_button = QPushButton("üíæ ‰øùÂ≠òÈÖçÁΩÆ")
        self.save_button.clicked.connect(self.manual_save)
        layout.addSpacing(16)  # ‚úÖ Ê∑ªÂä†ÊåâÈíÆ‰∏é‰∏äÊñπÁöÑÈó¥Ë∑ù
        layout.addWidget(self.save_button)

        self.config_tab.setLayout(layout)

    def setup_about_tab(self):  # ÂÖ≥‰∫éÊàë‰ª¨È°µÈù¢
        layout = QVBoxLayout()
        layout.addStretch(1)

        # ÊûÑÂª∫Â§ñÈÉ®Âç°ÁâáÊ°Ü
        frame = QFrame()
        frame.setStyleSheet("""
            QFrame {
                border-radius: 15px;  /* ËÆæÁΩÆÂúÜËßíËæπÊ°ÜÔºåÂçäÂæÑ‰∏∫15ÂÉèÁ¥†Ôºå‰ΩøËæπËßíÂúÜÊ∂¶ */
                padding: 25px;        /* ÂÜÖËæπË∑ùÔºåËÆ©ÂÜÖÂÆπ‰∏éËæπÊ°ÜÊúâË∑ùÁ¶ªÔºåÊõ¥ÁæéËßÇ */
                margin-left: 15px;    /* Â∑¶ËæπË∑ùÔºåÊéßÂà∂Êï¥‰∏™Âç°ÁâáË∑ùÁ¶ªÂÆπÂô®ËæπÁöÑÁ©∫Èöô */
                margin-right: 15px;   /* Âè≥ËæπË∑ùÔºåÁ°Æ‰øùÊï¥‰ΩìÂ±Ö‰∏≠ÂØπÁß∞ */

                background: qlineargradient(  /* ËÆæÁΩÆÁ∫øÊÄßÊ∏êÂèòËÉåÊôØ */
                    spread:pad,               /* Ê∏êÂèòÂ°´ÂÖÖÊ®°ÂºèÔºöpad Ë°®Á§∫ËæπÁºò‰ª•ÊúÄÂêé‰∏Ä‰∏™È¢úËâ≤Â°´ÂÖÖ */
