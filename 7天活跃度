    def set_default_dates(self):  # ËÆæÁΩÆÈªòËÆ§Êó•Êúü‰∏∫‰ªäÂ§©ÂíåÊòéÂ§©
        current_date = datetime.now().date()
        tomorrow = current_date + timedelta(days=1)
        self.start_time_input.setDateTime(QDateTime(current_date.year, current_date.month, current_date.day, 0, 0))
        self.end_time_input.setDateTime(QDateTime(tomorrow.year, tomorrow.month, tomorrow.day, 0, 0))

    def move_to_previous_day(self):  # ÁßªÂä®Âà∞Ââç‰∏ÄÂ§©
        da = self.start_time_input.dateTime().addDays(-7)
        date = self.start_time_input.dateTime().addDays(-1)
        self.start_time_input.setDateTime(da)
        self.end_time_input.setDateTime(date.addDays(1))

    def move_to_next_day(self):  # ÁßªÂä®Âà∞Âêé‰∏ÄÂ§©
        date = self.start_time_input.dateTime().addDays(1)
        self.start_time_input.setDateTime(date)
        self.end_time_input.setDateTime(date.addDays(1))

    def select_file(self):  # ÈÄâÊã©Áî®Êà∑IDÊñá‰ª∂
        desktop_path = QStandardPaths.writableLocation(QStandardPaths.DesktopLocation)  # Ëé∑ÂèñÊ°åÈù¢Ë∑ØÂæÑ
        file_name, _ = QFileDialog.getOpenFileName(self, "ÈÄâÊã©Áî®Êà∑IDÊñá‰ª∂", desktop_path,
                                                   "Text Files (*.txt);;All Files (*)")
        if file_name:
            error_lines = []  # Áî®‰∫éËÆ∞ÂΩïÂåÖÂê´ÈùûÊï∞Â≠óÂ≠óÁ¨¶ÁöÑË°åÂè∑
            self.user_ids = []  # Ê∏ÖÁ©∫‰πãÂâçÁöÑÁî®Êà∑IDÂàóË°®

            try:
                with open(file_name, 'r', encoding='utf-8') as file:  # ‰ΩøÁî®utf-8ÁºñÁ†ÅÊâìÂºÄÊñá‰ª∂
                    for line_number, line in enumerate(file, start=1):  # ÈÄêË°åËØªÂèñÊñá‰ª∂
                        line = line.strip()  # ÂéªÈô§Á©∫ÁôΩÂ≠óÁ¨¶
                        if line:  # Â¶ÇÊûúË°å‰∏ç‰∏∫Á©∫
                            if line.isdigit():  # Ê£ÄÊü•ÊòØÂê¶‰∏∫Á∫ØÊï∞Â≠ó
                                self.user_ids.append(line)  # Â¶ÇÊûúÊòØÊï∞Â≠óÔºåÊ∑ªÂä†Âà∞Áî®Êà∑IDÂàóË°®
                            else:
                                error_lines.append(line_number)  # ËÆ∞ÂΩïÈîôËØØË°åÂè∑

                if error_lines:
                    # Â¶ÇÊûúÊúâÈîôËØØË°åÔºåÊèêÁ§∫Áî®Êà∑
                    error_message = f"Êñá‰ª∂‰∏≠ÊúâÈùûÊï∞Â≠óÂ≠óÁ¨¶ÔºåÈîôËØØË°åÂè∑Ôºö{', '.join(map(str, error_lines))}"
                    QMessageBox.warning(self, "ÈîôËØØ", error_message)
                else:
                    # Â¶ÇÊûúÊ≤°ÊúâÈîôËØØÔºåÊèêÁ§∫Âä†ËΩΩÊàêÂäü
                    QMessageBox.information(self, "ÂØºÂÖ•ÊàêÂäü", f"ÊàêÂäüÂØºÂÖ• {len(self.user_ids)} ‰∏™Áî®Êà∑IDÔºÅ")
                    self.get_button.setEnabled(True)

            except UnicodeDecodeError:
                # Â¶ÇÊûúÊñá‰ª∂ÁºñÁ†ÅÈîôËØØÔºåÊèêÁ§∫Áî®Êà∑
                QMessageBox.warning(self, "ÈîôËØØ", "Êñá‰ª∂ÁºñÁ†ÅÈîôËØØÔºåËØ∑Á°Æ‰øùÊñá‰ª∂‰ΩøÁî®UTF-8ÁºñÁ†ÅÔºÅ")

    def start_processing(self):  # ÂºÄÂßãÂ§ÑÁêÜÊï∞ÊçÆ
        # Ê£ÄÊü•ÊòØÂê¶ÈÄâÊã©‰∫ÜÊñá‰ª∂
        if not hasattr(self, 'user_ids') or not self.user_ids:
            QMessageBox.warning(self, "ÊèêÁ§∫", "ËØ∑ÂÖàÈÄâÊã©ÂåÖÂê´Áî®Êà∑IDÁöÑÊñá‰ª∂ÔºÅ")
            return

        self.completed_tasks = []  # Áî®‰∫éÂ≠òÂÇ®Â∑≤ÂÆåÊàêÁöÑ‰ªªÂä°
        port = self.port_input.text().strip()
        group = self.group_select_home.currentText()
        token = self.token_inputs[group].text().strip()
        code = self.code_input.text().strip()

        missing_fields = []
        if not port:
            missing_fields.append("Á´ØÂè£ÔºàportÔºâ")
        if not code:
            missing_fields.append("È™åËØÅÁ†ÅÔºàcodeÔºâ")
        if not token:
            missing_fields.append(f"Â∞èÁªÑ {group} ÁöÑ Cookie")

        if missing_fields:
            QMessageBox.warning(self, "ËæìÂÖ•ÈîôËØØ", f"ËØ∑Â°´ÂÜô‰ª•‰∏ãÂÜÖÂÆπÔºö\n" + "\n".join(missing_fields))
            return

        # Ê£ÄÊü•Êó∂Èó¥ËæìÂÖ•
        start = self.start_time_input.dateTime().toString("yyyy-MM-dd HH:mm:ss")
        end = self.end_time_input.dateTime().toString("yyyy-MM-dd HH:mm:ss")
        if end < start:
            QMessageBox.warning(self, 'Ë≠¶Âëä', 'ÁªìÊùüÊó∂Èó¥‰∏çËÉΩÊó©‰∫éÂºÄÂßãÊó∂Èó¥')
            return

        # Â∞ÜÂ≠óÁ¨¶‰∏≤ËΩ¨Êç¢‰∏∫datetimeÂØπË±°
        start_datetime = datetime.strptime(start, '%Y-%m-%d %H:%M:%S')
        end_datetime = datetime.strptime(end, '%Y-%m-%d %H:%M:%S')
        print(f"ÂºÄÂßãÊó∂Èó¥: {start_datetime}, ÁªìÊùüÊó∂Èó¥: {end_datetime}")

        # Â∞ÜÊó∂Èó¥ËΩ¨Êç¢‰∏∫Êó∂Èó¥Êà≥
        start_time = int(start_datetime.timestamp())
        end_time = int(end_datetime.timestamp())

        # Ëé∑ÂèñÁî®Êà∑ËæìÂÖ•ÁöÑÁ∫øÁ®ãÊï∞Èáè
        thread_count = self.thread_count_input.text()
        if thread_count.isdigit() and int(thread_count) > 0:
            max_threads = min(int(thread_count), 100)  # ÈôêÂà∂Á∫øÁ®ãÊï∞ÈáèÊúÄÈ´ò‰∏∫100
            self.thread_pool.setMaxThreadCount(max_threads)
        else:
            QMessageBox.warning(self, "ËæìÂÖ•ÈîôËØØ", "ËØ∑Á°Æ‰øùÂ§öÂºÄÊï∞Èáè‰∏∫Ê≠£Êï¥Êï∞ÔºÅ")
            return

        # Êõ¥Êñ∞Áä∂ÊÄÅÊ†è
        self.status_label.setText(f"Áä∂ÊÄÅÔºöÊ≠£Âú®Ëé∑ÂèñÊï∞ÊçÆ (0/{len(self.user_ids)})")
        self.progress_bar.setMaximum(len(self.user_ids))
        self.progress_bar.setValue(0)
        self.progress_bar.setVisible(True)

        for user_id in self.user_ids:
            signals = WorkerSignals()
            signals.finished.connect(self.task_finished)
            worker = DataWorker(user_id, signals, port, token, code, start_time, end_time)
            self.thread_pool.start(worker)

            self.file_button.setEnabled(False)  # Á¶ÅÁî®ÈÄâÊã©Êñá‰ª∂ÊåâÈíÆ
            self.thread_count_input.setEnabled(False)  # Á¶ÅÁî®Á∫øÁ®ãËæìÂÖ•
            self.group_select_home.setEnabled(False)  # Á¶ÅÁî®Â∞èÁªÑÈÄâÊã©ÈÄâÊã©
            self.start_time_input.setEnabled(False)  # Á¶ÅÁî®ÂºÄÂßãÊó∂Èó¥
            self.end_time_input.setEnabled(False)  # Á¶ÅÁî®ÁªìÊùüÊåâÈíÆ
            self.get_button.setEnabled(False)  # Á¶ÅÁî®Ëé∑ÂèñÊåâÈíÆ
            self.export_button.setEnabled(False)  # Á¶ÅÁî®ÂØºÂá∫ÊåâÈíÆ
            self.prev_day_btn.setEnabled(False)  # Á¶ÅÁî®‰∏ä‰∏ÄÊó•ÊåâÈíÆ
            self.today_btn.setEnabled(False)  # Á¶ÅÁî®ÂΩìÊó•ÊåâÈíÆ
            self.next_day_btn.setEnabled(False)  # Á¶ÅÁî®‰∏ã‰∏ÄÊó•ÊåâÈíÆ

    def task_finished(self, result):  # ‰ªªÂä°ÂÆåÊàêÁöÑÂõûË∞ÉÂáΩÊï∞
        self.completed_tasks.append(result)
        # Êõ¥Êñ∞ËøõÂ∫¶Êù°ÂíåÁä∂ÊÄÅ
        progress = len(self.completed_tasks)
        total = len(self.user_ids)
        self.progress_bar.setValue(progress)
        self.status_label.setText(f"Áä∂ÊÄÅÔºöÊ≠£Âú®Ëé∑ÂèñÊï∞ÊçÆ ({progress}/{total})")

        if progress == total:
            self.status_label.setText("Áä∂ÊÄÅÔºöÊâÄÊúâÊï∞ÊçÆÂ∑≤Ëé∑ÂèñÂÆåÊàêÔºÅ")
            self.progress_bar.setVisible(False)

        if len(self.completed_tasks) == len(self.user_ids):
            QMessageBox.information(self, "ÊèêÁ§∫", f"ÊâÄÊúâÊï∞ÊçÆÂ∑≤Ëé∑ÂèñÂÆåÊàêÔºÅ")
            self.file_button.setEnabled(True)  # ÂêØÁî®ÈÄâÊã©Êñá‰ª∂ÊåâÈíÆ
            self.thread_count_input.setEnabled(True)  # ÂêØÁî®Á∫øÁ®ãËæìÂÖ•
            self.group_select_home.setEnabled(True)  # ÂêØÁî®Â∞èÁªÑÈÄâÊã©ÈÄâÊã©
            self.start_time_input.setEnabled(True)  # ÂêØÁî®ÂºÄÂßãÊó∂Èó¥
            self.end_time_input.setEnabled(True)  # ÂêØÁî®ÁªìÊùüÊåâÈíÆ
            self.get_button.setEnabled(True)  # ÂêØÁî®Ëé∑ÂèñÊåâÈíÆ
            self.export_button.setEnabled(True)  # ÂêØÁî®ÂØºÂá∫ÊåâÈíÆ
            self.prev_day_btn.setEnabled(True)  # ÂêØÁî®‰∏ä‰∏ÄÊó•ÊåâÈíÆ
            self.today_btn.setEnabled(True)  # ÂêØÁî®ÂΩìÊó•ÊåâÈíÆ
            self.next_day_btn.setEnabled(True)  # ÂêØÁî®‰∏ã‰∏ÄÊó•ÊåâÈíÆ

    def export_data(self):  # ÂØºÂá∫Êï∞ÊçÆÂà∞ExcelÊñá‰ª∂
        if not self.completed_tasks:
            QMessageBox.warning(self, "Ë≠¶Âëä", "Ê≤°ÊúâÊï∞ÊçÆÂèØÂØºÂá∫ÔºÅ")
            return

        try:
            wb = Workbook()
            ws = wb.active
            ws.append(['APPID', 'ÊúâÊïàÊäïÊ≥®', 'ÂÖ∂‰ªñ‰ø°ÊÅØ', 'Ê≥®ÂÜåIP', 'ÊúÄÂêéÁôªÂΩïIP', 'Ê≥®ÂÜåÊó∂Èó¥', 'ÊúÄÂêéÁôªÂΩïÊó∂Èó¥', 'ÊèêÂÖÖÂÆ¢Êçü',
                       'ÂÖÖÂÄº', 'ÊèêÁé∞', 'Ê∏∏ÊàèÊçüÁõä', 'Ê∏∏ÊàèID', 'Ê∏†ÈÅì‰ø°ÊÅØ'])  # Êõ¥Êñ∞Ë°®Â§¥

            for task in self.completed_tasks:
                parts = task.split(", ")
                # ‰ΩøÁî®ÈªòËÆ§ÂÄºÂ§ÑÁêÜÂèØËÉΩÁº∫Â§±ÁöÑÊï∞ÊçÆ
                Ê∏∏Êàèid = self.convert_to_number(parts[0]) if len(parts) > 0 else "Êó†Êï∞ÊçÆ"
                ÊúâÊïàÊäïÊ≥® = self.convert_to_number(parts[1]) if len(parts) > 1 else "Êó†Êï∞ÊçÆ"
                ÂÖ∂‰ªñ‰ø°ÊÅØ = parts[2] if len(parts) > 2 else "Êó†Êï∞ÊçÆ"  # ÂêàÂπ∂ÁöÑÂÖ∂‰ªñ‰ø°ÊÅØ
                Ê≥®ÂÜåIP = parts[3] if len(parts) > 3 else "Êó†Êï∞ÊçÆ"
                ÊúÄÂêéÁôªÂΩïIP = parts[4] if len(parts) > 4 else "Êó†Êï∞ÊçÆ"
                Ê≥®ÂÜåÊó∂Èó¥ = parts[5] if len(parts) > 5 else "Êó†Êï∞ÊçÆ"
                ÊúÄÂêéÁôªÂΩïÊó∂Èó¥ = parts[6] if len(parts) > 6 else "Êó†Êï∞ÊçÆ"
                ÊèêÂÖÖÂÆ¢Êçü = self.convert_to_number(parts[7]) if len(parts) > 7 else "Êó†Êï∞ÊçÆ"
                ÂÖÖÂÄº = self.convert_to_number(parts[8]) if len(parts) > 8 else "Êó†Êï∞ÊçÆ"
                ÊèêÁé∞ = self.convert_to_number(parts[9]) if len(parts) > 9 else "Êó†Êï∞ÊçÆ"
                Ê∏∏ÊàèÊçüÁõä = self.convert_to_number(parts[10]) if len(parts) > 10 else "Êó†Êï∞ÊçÆ"
                APPID = self.convert_to_number(parts[11]) if len(parts) > 11 else "Êó†Êï∞ÊçÆ"
                Ê∏†ÈÅì‰ø°ÊÅØ = parts[12] if len(parts) > 12 else "Êó†Êï∞ÊçÆ"  # ÂêàÂπ∂ÁöÑÊ∏†ÈÅì‰ø°ÊÅØ

                # Êõ¥Êñ∞Êï∞ÊçÆË°å
                ws.append(
                    [APPID, ÊúâÊïàÊäïÊ≥®, ÂÖ∂‰ªñ‰ø°ÊÅØ, Ê≥®ÂÜåIP, ÊúÄÂêéÁôªÂΩïIP, Ê≥®ÂÜåÊó∂Èó¥, ÊúÄÂêéÁôªÂΩïÊó∂Èó¥, ÊèêÂÖÖÂÆ¢Êçü,
                     ÂÖÖÂÄº, ÊèêÁé∞, Ê∏∏ÊàèÊçüÁõä, Ê∏∏Êàèid, Ê∏†ÈÅì‰ø°ÊÅØ])

            current_time = datetime.now().strftime("%YÂπ¥%mÊúà%dÊó•%HÊó∂%MÂàÜ%SÁßí")
            filename = f"ÂØºÂá∫Êï∞ÊçÆ_{current_time}.xlsx"
            wb.save(filename)
            QMessageBox.information(self, "ÊèêÁ§∫", f"Êï∞ÊçÆÂ∑≤ÂØºÂá∫Âà∞ {filename}")
            os.startfile(filename)

        except Exception as e:
            QMessageBox.critical(self, "ÂØºÂá∫ÈîôËØØ", f"ÂØºÂá∫Êï∞ÊçÆÊó∂ÂèëÁîüÈîôËØØ: {str(e)}")

    def convert_to_number(self, value):  # Â∞ÜÂ≠óÁ¨¶‰∏≤ËΩ¨Êç¢‰∏∫Êï∞Â≠óÔºåÂ¶ÇÊûúËΩ¨Êç¢Â§±Ë¥•ÂàôËøîÂõû"Êó†Êï∞ÊçÆ"
        try:
            return float(value)  # Â∞ùËØïÂ∞ÜÂÄºËΩ¨Êç¢‰∏∫ÊµÆÁÇπÊï∞
        except ValueError:
            return "Êó†Êï∞ÊçÆ"  # ËΩ¨Êç¢Â§±Ë¥•Êó∂ËøîÂõû"Êó†Êï∞ÊçÆ"

    def load_group_names_from_config(self):
        config = ConfigParser()
        config.read('config.ini')
        if 'GROUP_COOKIES' in config:
            return list(config['GROUP_COOKIES'].keys())
        # ÈªòËÆ§ÁªÑÔºàÈ¶ñÊ¨°ËøêË°åÊàñ config.ini Â∞ö‰∏çÂ≠òÂú®Ôºâ
        return ["ÁªøËå∂", "Â¶ñÂß¨", "Ëâ≤Êàí", "Áà±Ëâ≤Êí≠", "ÊÄßÁ¶è", "Â∞ëÂ•≥", "‰∫∫Â¶ª", "‰πêÂí™"]

    def save_config(self, port, token_dict, code):
        config = ConfigParser()

        # ‰øùÂ≠òÈÄöÁî®ÈÖçÁΩÆ
        config['COMMON'] = {
            'Port': port,
            'Code': code
        }

        # ‰øùÂ≠òÂ∞èÁªÑCookieÂà∞Áªü‰∏ÄÂàÜÂå∫
        config['GROUP_COOKIES'] = {group: token for group, token in token_dict.items()}

        with open('config.ini', 'w') as configfile:
            config.write(configfile)

    def load_config(self):
        config = ConfigParser()
        config.read('config.ini')

        if 'COMMON' in config:
            self.port_input.setText(config['COMMON'].get('Port', ''))
            self.code_input.setText(config['COMMON'].get('Code', ''))

        if 'GROUP_COOKIES' in config:
            for group in self.groups:
                if group in config['GROUP_COOKIES']:
                    self.token_inputs[group].setText(config['GROUP_COOKIES'][group])

    def manual_save(self):  # ÊâãÂä®‰øùÂ≠òÈÖçÁΩÆ
        port = self.port_input.text()
        code = self.code_input.text()

        token_dict = {}
        missing_groups = []

        for group, input_field in self.token_inputs.items():
            token = input_field.text()
            if token:
                token_dict[group] = token
                input_field.setStyleSheet("")  # Ê∏ÖÈô§Á∫¢Ê°Ü
            else:
                missing_groups.append(group)
                input_field.setStyleSheet("border: 1px solid red;")

        if not port or not code:
            QMessageBox.warning(self, "Ë≠¶Âëä", "ËØ∑Â°´ÂÜôÂÆåÊï¥ÁöÑÈÖçÁΩÆÔºàportÂíåcodeÔºâÔºÅ")
            return

        self.save_config(port, token_dict, code)

        if missing_groups:
            missing_text = "„ÄÅ".join(missing_groups)
            QMessageBox.information(self, "ÈÉ®ÂàÜ‰øùÂ≠òÊàêÂäü", f"ÈÖçÁΩÆÂ∑≤‰øùÂ≠òÔºå‰ΩÜ‰ª•‰∏ãÂ∞èÁªÑÊú™Â°´ÂÜô CookieÔºö{missing_text}")
        else:
            QMessageBox.information(self, "ÊèêÁ§∫", "ÈÖçÁΩÆÂ∑≤ÂÆåÊï¥‰øùÂ≠òÔºÅ")


if __name__ == "__main__":
    app = QApplication(sys.argv)
    window = UserWindow()
    window.show()
    sys.exit(app.exec_())
