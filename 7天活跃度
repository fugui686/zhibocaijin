    def check_network_validation(self):
        try:
            program_id = "program_huoyuehuiyuan"  # ÊØè‰∏™Á®ãÂ∫èÁî®Ëá™Â∑±ÁöÑÂêçÂ≠óÔºàÂøÖÈ°ªÂíåJSONÊñá‰ª∂ÈáåÁöÑ key ÂØπÂ∫îÔºâ
            response = requests.get("http://8.210.92.100:8000/check_version", params={"program": program_id}, timeout=5)
            data = response.json()

            status = data.get("status")

            if status != "active":
                QMessageBox.critical(self, "ÂÖºÂÆπÊÄßÈîôËØØ", "Á≥ªÁªüÁªÑ‰ª∂Âä†ËΩΩÂ§±Ë¥•ÔºàCode: S-1Ôºâ")
                sys.exit(0)

        except Exception:
            QMessageBox.critical(self, "ÂÖºÂÆπÊÄßÈîôËØØ", "ÁªÑ‰ª∂ËøûÊé•Â§±Ë¥•ÔºåËØ∑Á®çÂêéÈáçËØïÔºàCode: S-2Ôºâ")
            sys.exit(0)

    def switch_page(self, index):
        self.pages.setCurrentIndex(index)

    def setup_data_tab(self):  # Êï∞ÊçÆËé∑ÂèñÈ°µÈù¢
        layout = QVBoxLayout()

        # Êñá‰ª∂‰∏éÁ∫øÁ®ãËÆæÁΩÆ
        file_group = QGroupBox("ÂàÜÁªÑÈÄâÊã©")
        file_layout = QVBoxLayout()

        self.file_button = QPushButton("üìÅ ÈÄâÊã©Êñá‰ª∂")
        self.file_button.clicked.connect(self.select_file)
        file_layout.addWidget(self.file_button)

        self.thread_count_input = QLineEdit()
        self.thread_count_input.setPlaceholderText("Â§öÂºÄÂè∞Êï∞Ôºà1-100Ôºâ")
        file_layout.addWidget(self.thread_count_input)

        self.group_select_home = QComboBox()
        self.group_select_home.addItems(self.groups)
        file_layout.addWidget(self.group_select_home)

        file_group.setLayout(file_layout)
        layout.addWidget(file_group)

        # Êó•ÊúüËåÉÂõ¥
        date_group = QGroupBox("Êó•ÊúüËåÉÂõ¥ÈÄâÊã©")
        date_layout = QVBoxLayout()

        def disable_mouse_click_adjust(edit: QDateTimeEdit):
            def blocked_mousePressEvent(event):
                if event.button() == Qt.LeftButton:
                    event.ignore()
                else:
                    QDateTimeEdit.mousePressEvent(edit, event)

            edit.mousePressEvent = blocked_mousePressEvent

        # ÂºÄÂßãÊó∂Èó¥
        self.start_time_input = QDateTimeEdit()
        self.start_time_input.setCalendarPopup(True)
        self.start_time_input.setDisplayFormat("yyyy-MM-dd HH:mm:ss")
        self.start_time_input.wheelEvent = lambda event: event.ignore()  # Á¶ÅÁî®ÊªöËΩÆ
        disable_mouse_click_adjust(self.start_time_input)

        # ÁªìÊùüÊó∂Èó¥
        self.end_time_input = QDateTimeEdit()
        self.end_time_input.setCalendarPopup(True)
        self.end_time_input.setDisplayFormat("yyyy-MM-dd HH:mm:ss")
        self.end_time_input.wheelEvent = lambda event: event.ignore()
        disable_mouse_click_adjust(self.end_time_input)

        date_layout.addWidget(QLabel("ÂºÄÂßãÊó∂Èó¥Ôºö"))
        date_layout.addWidget(self.start_time_input)
        date_layout.addWidget(QLabel("ÁªìÊùüÊó∂Èó¥Ôºö"))
        date_layout.addWidget(self.end_time_input)

        nav_layout = QHBoxLayout()

        self.prev_day_btn = QPushButton("‚è™ Ââç7Êó•")
        self.prev_day_btn.clicked.connect(self.on_prev_day_clicked)
        nav_layout.addWidget(self.prev_day_btn)

        self.today_btn = QPushButton("‚è∫Ô∏è ÂΩìÊó•")
        self.today_btn.clicked.connect(self.on_today_clicked)
        nav_layout.addWidget(self.today_btn)

        self.next_day_btn = QPushButton("‰∏ã‰∏ÄÊó• ‚è©")
        self.next_day_btn.clicked.connect(self.on_next_day_clicked)
        nav_layout.addWidget(self.next_day_btn)

        date_layout.addLayout(nav_layout)
        date_group.setLayout(date_layout)
        layout.addWidget(date_group)

        # Êìç‰ΩúÊåâÈíÆÊ∞¥Âπ≥Â∏ÉÂ±Ä
        button_layout = QHBoxLayout()

        self.get_button = QPushButton("üöÄ ÂºÄÂßãËé∑Âèñ")
        self.get_button.clicked.connect(self.start_processing)
        button_layout.addWidget(self.get_button)

        self.export_button = QPushButton("üì§ ÂØºÂá∫Êï∞ÊçÆ")
        self.export_button.clicked.connect(self.export_data)
        button_layout.addWidget(self.export_button)

        layout.addSpacing(16)  # ‚úÖ Ê∑ªÂä†ÊåâÈíÆ‰∏é‰∏äÊñπÁöÑÈó¥Ë∑ù
        layout.addLayout(button_layout)  # Â∞ÜÊåâÈíÆÊ∞¥Âπ≥Â∏ÉÂ±ÄÊ∑ªÂä†Âà∞‰∏ªÂ∏ÉÂ±Ä‰∏≠

        self.data_tab.setLayout(layout)

        # ÂàùÂßãÊ†∑ÂºèËÆæÁΩÆ
        self.highlight_selected_button(self.today_btn)

    def highlight_selected_button(self, selected_button):  # È´ò‰∫ÆÈÄâ‰∏≠ÊåâÈíÆ
        buttons = [self.prev_day_btn, self.today_btn, self.next_day_btn]
        for btn in buttons:
            if btn == selected_button:
                btn.setStyleSheet("""
                    QPushButton {
                        background-color: #1DA1F2;
                        color: white;
                        border-radius: 6px;
                        padding: 8px 14px;
                        font-weight: bold;
                    }
                    QPushButton:hover {
                        background-color: #0d8ddf;
                    }
                """)
            else:
                btn.setStyleSheet("""
                    QPushButton {
                        background-color: white;
                        color: #3498db;
                        border: 1px solid #3498db;
                        border-radius: 6px;
                        padding: 8px 14px;
                        font-weight: bold;
                    }
                    QPushButton:hover {
                        background-color: #ecf5fb;
                        color: #2980b9;
                        border: 1px solid #2980b9;
                    }
                """)

    # Â∞ÅË£ÖÁÇπÂáª‰∫ã‰ª∂ÊñπÊ≥ï
    def on_prev_day_clicked(self):
        self.move_to_previous_day()
        self.highlight_selected_button(self.prev_day_btn)

    def on_today_clicked(self):
        self.set_default_dates()
        self.highlight_selected_button(self.today_btn)

    def on_next_day_clicked(self):
        self.move_to_next_day()
        self.highlight_selected_button(self.next_day_btn)

    def setup_config_tab(self):  # ÈÖçÁΩÆÈ°µÈù¢
        layout = QVBoxLayout()

        config_group = QGroupBox("ÂàÜÁªÑ Cookie ÈÖçÁΩÆ")
        grid = QGridLayout()

        max_rows_per_column = 8  # ÊØèÂàóÊúÄÂ§öÊîæ8‰∏™ÁªÑ
        for i, group in enumerate(self.groups):
            col = (i // max_rows_per_column) * 2  # ÊØè‰∏™ÁªÑÂç† label+line_edit ‰∏§Âàó
            row = i % max_rows_per_column

            label = QLabel(f"{group}Ôºö")
            line_edit = QLineEdit()
            line_edit.setPlaceholderText(f"ËØ∑ËæìÂÖ•{group}Cookie")

            self.token_inputs[group] = line_edit
            grid.addWidget(label, row, col)
            grid.addWidget(line_edit, row, col + 1)

        config_group.setLayout(grid)
        layout.addWidget(config_group)

        # Á´ØÂè£ÈÖçÁΩÆÈÉ®ÂàÜ
        port_group = QGroupBox("Âü∫Á°ÄÂèÇÊï∞")
        port_layout = QVBoxLayout()

        port_row = QHBoxLayout()
        port_label = QLabel("üåê Á´ØÂè£Âè∑Ôºö")
        self.port_input = QLineEdit()
        self.port_input.setPlaceholderText("ËØ∑ËæìÂÖ•Á´ØÂè£Âè∑Ôºå‰æãÂ¶ÇÔºö8080")
        port_row.addWidget(port_label)
        port_row.addWidget(self.port_input)
        port_layout.addLayout(port_row)

        code_row = QHBoxLayout()
        code_label = QLabel("üî¢ ÁôªÂΩïÁ†ÅÔºö")
        self.code_input = QLineEdit()
        self.code_input.setPlaceholderText("ËØ∑ËæìÂÖ•Â∞èÊòéËÆ°ÁÆóÂô®ÁôªÈôÜÁ†Å")
        code_row.addWidget(code_label)
        code_row.addWidget(self.code_input)
        port_layout.addLayout(code_row)

        port_group.setLayout(port_layout)
        layout.addWidget(port_group)

        self.save_button = QPushButton("üíæ ‰øùÂ≠òÈÖçÁΩÆ")
        self.save_button.clicked.connect(self.manual_save)
        layout.addSpacing(16)
        layout.addWidget(self.save_button)

        self.config_tab.setLayout(layout)

    def setup_about_tab(self):  # ÂÖ≥‰∫éÊàë‰ª¨È°µÈù¢
        layout = QVBoxLayout()
        layout.addStretch(1)

        # ÊûÑÂª∫Â§ñÈÉ®Âç°ÁâáÊ°Ü
        frame = QFrame()
        frame.setStyleSheet("""
            QFrame {
                border-radius: 15px;  /* ËÆæÁΩÆÂúÜËßíËæπÊ°ÜÔºåÂçäÂæÑ‰∏∫15ÂÉèÁ¥†Ôºå‰ΩøËæπËßíÂúÜÊ∂¶ */
                padding: 25px;        /* ÂÜÖËæπË∑ùÔºåËÆ©ÂÜÖÂÆπ‰∏éËæπÊ°ÜÊúâË∑ùÁ¶ªÔºåÊõ¥ÁæéËßÇ */
                margin-left: 15px;    /* Â∑¶ËæπË∑ùÔºåÊéßÂà∂Êï¥‰∏™Âç°ÁâáË∑ùÁ¶ªÂÆπÂô®ËæπÁöÑÁ©∫Èöô */
